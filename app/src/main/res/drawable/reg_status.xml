<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/dark" android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
            <stroke android:width="1dp" android:color="@color/white"></stroke>
            <solid android:color="@color/unregistered"></solid>
            <size android:width="21dp" android:height="21dp"></size>
        </shape>
    </item>
    <item android:color="@color/dark" android:state_enabled="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
            <stroke android:width="1dp" android:color="@color/white"></stroke>
            <solid android:color="@color/registered"></solid>
            <size android:width="21dp" android:height="21dp"></size>
        </shape>
    </item>
</selector>